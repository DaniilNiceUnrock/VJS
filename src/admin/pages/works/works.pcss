.page-content {
  padding: 60px 0 20px;
  background: url("../../images/bg/admin.jpg") center center / cover no-repeat;
  flex: 1;
}
.cards {
  display: flex;
  flex-wrap: wrap;
  margin-left: -20px;
}

.item {
  width: calc(100% / 3 - 20px);
  margin-left: 20px;
}

.form {
  margin-bottom: 30px;
}
.header {
  display: flex;
  margin-bottom: 60px;
}
.title {
  margin-right: 60px;
  color: #2D3C4E;
  font-weight: bold;
  font-size: 21px;
}